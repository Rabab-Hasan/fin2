# Frontend Development Environment Configuration

# API Configuration - Local development URLs
REACT_APP_API_URL=http://localhost:2345
REACT_APP_API_BASE_URL=http://localhost:2345/api

# Security & Encryption
REACT_APP_ENCRYPTION_KEY=frontend-encryption-key-32-chars-long
REACT_APP_JWT_STORAGE_KEY=finance_dashboard_jwt

# Application Settings
REACT_APP_NAME=Finance Dashboard
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG_LOGS=true
REACT_APP_ENABLE_MARKETING_ANALYSIS=true
REACT_APP_ENABLE_CAMPAIGN_ASSISTANT=true

# Build Settings
GENERATE_SOURCEMAP=true
SKIP_PREFLIGHT_CHECK=true
